<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <!-- Plugin Meta Define File -->
        <PluginDFile>$(MSBuildThisFileDirectory)plugin.d.json</PluginDFile>
        <!-- Enable Default Exclude -->
        <DefaultExclude>true</DefaultExclude>
        <!-- Package Name -->
        <PluginPackagePath>$(ProjectDir)Packages\$(TargetName)-$(Version).zip</PluginPackagePath>
    </PropertyGroup>
</Project>